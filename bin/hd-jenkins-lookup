#! /usr/bin/perl

# PODNAME: hd-jenkins-lookup
# ABSTRACT: Collect and coordinate the build number information with branch names
use strict;
use warnings;
use Honeydew::Jenkins::BuildLookup;

my $jenkins = Honeydew::Jenkins::BuildLookup->new;
my $build_branches = $jenkins->get_build_branches;
$jenkins->store_build_branches( $build_branches );
